    "num = int(input())\n",
    "\n",
    "def evenSum(num) :\n",
    "    i=1\n",
    "    sum = 0\n",
    "    while i <= num :\n",
    "        if(i % 2 == 0) : \n",
    "            sum += i\n",
    "        i+=1\n",
    "    print(sum)\n",
    "\n",
