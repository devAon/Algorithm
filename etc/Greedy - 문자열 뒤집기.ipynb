    "s = input()\n",
    "\n",
    "count0 = 0\n",
    "count1 = 1\n",
    "\n",
    "if s[0] == '0':\n",
    "    count1 += 1\n",
    "else:\n",
    "    count0 += 1\n",
    "\n",
    "for i in range(len(s)-1):\n",
    "    if s[i] != s[i+1]:\n",
    "        if s[i+1] == '1':\n",
    "            count0 += 1\n",
    "        else:\n",
    "            count1 += 1\n",
    "print(min(count0, count1))"
