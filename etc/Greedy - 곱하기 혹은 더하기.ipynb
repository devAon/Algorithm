    "\n",
    "result = int(s[0])\n",
    "for i in range(1, len(s)):\n",
    "    num = int(s[i])\n",
    "    if result <= 1 or num <= 1:\n",
    "        result += num\n",
    "    else:\n",
    "        result *= num\n",
    "print(result)"
