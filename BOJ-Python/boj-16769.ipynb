    "C, M = list(), list()\n",
    "for i in range(3):\n",
    "    a, b = map(int, input().split())\n",
    "    C.append(a)\n",
    "    M.append(b)\n",
    "for i in range(100):\n",
    "    idx = i % 3\n",
    "    nxt = (idx + 1) % 3\n",
    "    M[idx], M[nxt] = max(M[idx]-(C[nxt] - M[nxt]), 0), min(C[nxt], M[nxt] + M[idx])\n",
    "for i in M:\n",
    "    print(i)"
   ]
